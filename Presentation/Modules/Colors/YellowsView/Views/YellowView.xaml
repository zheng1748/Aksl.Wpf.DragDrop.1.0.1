             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d"
             d:DesignWidth="500"
             d:DesignHeight="500"
             prism:ViewModelLocator.AutoWireViewModel="True">
